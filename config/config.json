{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "diwug-command-action-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/diwugCommandAction/DiwugCommandActionCommandSet.js",
          "manifest": "./src/extensions/diwugCommandAction/DiwugCommandActionCommandSet.manifest.json"
        }
      ]
    },
    "copy-paste-footer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/copyPasteFooter/CopyPasteFooterApplicationCustomizer.js",
          "manifest": "./src/extensions/copyPasteFooter/CopyPasteFooterApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DiwugCommandActionCommandSetStrings": "lib/extensions/diwugCommandAction/loc/{locale}.js",
    "CopyPasteFooterApplicationCustomizerStrings": "lib/extensions/copyPasteFooter/loc/{locale}.js"
  }
}
